class UnitTestMinimalLexer
{
    private $_counter;
    private $_data;
    private $line;
    private $state = 1;
    public $token;
    public $value;

    function __construct($data) {
        $this -> _data = $data;
        $this -> _counter = 0;
        $this -> line = 1;
    }
    
    function getState() {
        return $this -> state;
    }

/*!lex2php
%counter $this->_counter
%input $this->_data
%line $this->line
%token $this->token
%value $this->value
*/
}